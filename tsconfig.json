{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES5",
        "sourceMap": true,
        "outDir": "JavaScript"
    },
    "include": [
        "TypeScript/**/*"
    ],
    "compileOnSave": true,
    "exclude": [
        "node_modules"
    ],
    "strict-comparisons": [
        true,
        {
            "allow-object-equal-comparison": false,
            "allow-string-order-comparison": false,
            "strict-comparisons": true
        }
    ]
}